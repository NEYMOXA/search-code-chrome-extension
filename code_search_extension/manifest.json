{
  "manifest_version": 3,
  "name": "Поиск кода",
  "version": "0.0.2",
  "description": "Помогает программистам искать код",
  "icons": {
    "48": "logo.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action":{
    "default_popup": "index.html",
    "default_icon": "logo.png"
  },
  "permissions": [
  "contextMenus",
  "storage",
  "tabs"
]
}